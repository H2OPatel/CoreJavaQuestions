package com.interview.question.clients;

import com.interview.question.methods.MostRepeatedWord;

public class MostRepeatedWordClient {

	public static void main(String[] args) {
		
		String line = "jal jatan john james john jatan jatan";
		String mostRepeatedWord = MostRepeatedWord.mostRepeatedWord(line);
		System.out.println("The Most repeated woed from given statement is : " + mostRepeatedWord);
		

	}

}
